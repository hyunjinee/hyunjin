{
  "name": "@hyunjin/blog",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "24.x"
  },
  "scripts": {
    "start": "next dev",
    "dev": "cross-env INIT_CWD=$PWD next dev",
    "dev:no-turbo": "cross-env INIT_CWD=$PWD next dev --turbo=false",
    "build": "cross-env INIT_CWD=$PWD contentlayer2 build && cross-env INIT_CWD=$PWD next build && cross-env NODE_OPTIONS='--experimental-json-modules' node ./scripts/postbuild.mjs",
    "serve": "next start",
    "analyze": "cross-env ANALYZE=true next build",
    "lint": "next lint --fix --dir pages --dir app --dir components --dir lib --dir layouts --dir scripts",
    "prepare": "husky",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@headlessui/react": "2.2.9",
    "@mdx-js/loader": "3.1.1",
    "@next/bundle-analyzer": "16.1.1",
    "@next/mdx": "16.1.1",
    "@tailwindcss/forms": "0.5.7",
    "@tailwindcss/postcss": "4.1.1",
    "@tailwindcss/typography": "0.5.19",
    "@vercel/analytics": "1.4.1",
    "@vercel/speed-insights": "1.1.0",
    "body-scroll-lock": "4.0.0-beta.0",
    "contentlayer2": "0.5.3",
    "esbuild": "0.27.2",
    "github-slugger": "2.0.0",
    "gray-matter": "4.0.2",
    "hast-util-from-html-isomorphic": "2.0.0",
    "image-size": "1.0.0",
    "katex": "0.16.27",
    "next": "16.1.1",
    "next-contentlayer2": "0.5.3",
    "next-themes": "0.4.4",
    "pdfjs-dist": "5.4.530",
    "pliny": "0.4.0",
    "postcss": "8.4.24",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-pdf": "^10.3.0",
    "reading-time": "1.5.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-citation": "^2.0.0",
    "rehype-katex": "^7.0.0",
    "rehype-preset-minify": "7.0.0",
    "rehype-prism-plus": "^2.0.0",
    "rehype-slug": "^6.0.0",
    "remark": "^15.0.0",
    "remark-gfm": "^4.0.0",
    "remark-github-blockquote-alert": "^1.2.1",
    "remark-math": "^6.0.0",
    "sharp": "0.34.1",
    "sugar-high": "^0.9.5",
    "tailwindcss": "4.1.1",
    "unist-util-visit": "^5.0.0",
    "zod": "4.2.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "3.2.4",
    "@storybook/addon-essentials": "8.5.3",
    "@storybook/addon-interactions": "8.5.3",
    "@storybook/addon-onboarding": "8.5.3",
    "@storybook/blocks": "8.5.3",
    "@storybook/nextjs": "8.5.3",
    "@storybook/react": "8.5.3",
    "@storybook/test": "8.5.3",
    "@svgr/webpack": "8.0.1",
    "@types/mdx": "2.0.12",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@typescript-eslint/eslint-plugin": "6.1.0",
    "@typescript-eslint/parser": "6.1.0",
    "babel-plugin-react-compiler": "19.0.0-beta-ebf51a3-20250411",
    "baseline-browser-mapping": "2.9.11",
    "cross-env": "7.0.3",
    "eslint": "8.45.0",
    "eslint-config-next": "16.1.1",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-storybook": "0.11.2",
    "husky": "9.0.0",
    "lint-staged": "13.0.0",
    "prettier": "3.0.0",
    "prettier-plugin-tailwindcss": "0.7.2",
    "storybook": "8.5.3",
    "typescript": "5.9.3"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
  }
}
